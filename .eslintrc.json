{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "plugin:prettier/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
          "modules": true
        }
    },
    "rules": {
        "react/prop-types": [1],
        "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}]
    }
}
